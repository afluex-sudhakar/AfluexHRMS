                    },
                    grid: { borderColor: "#FFCCD2", strokeDashArray: 4, yaxis: { lines: { show: true } } }
                }
                var chart1 = new ApexCharts(document.querySelector("#marketchart"), options1); chart1.render();

            }
        });
    };


  
