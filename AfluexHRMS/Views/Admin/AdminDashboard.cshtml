                        <div class="card-body">
                            <div class="row">
                                <div class="col">
                                    <h6 class="">TOTAL EMPLOYEES</h6>
                                    <h3 class="mb-2 number-font">@ViewBag.TotalEmployee</h3>
                                </div>
                                <div class="col col-auto">
                                    <div class="counter-icon dashboard_icon bg-primary-gradient box-shadow-primary brround mb-0 ms-auto">
                                        <img src="~/images/1.gif" class="rounded p-1" />
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-6 col-md-12 col-sm-12 col-xl-3">
