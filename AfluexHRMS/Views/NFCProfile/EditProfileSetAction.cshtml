            if (window.FormData !== undefined) {
                for (var x = 0; x < files.length; x++) {
                    data.append("file" + x, files[x]);
                }
                data.append("PK_ProfileId", PK_ProfileId);
                $.ajax({
                    url: '/NFCProfile/UpdateBannerImage/',
                    type: 'post',
                    datatype: 'json',
                    contentType: false,
                    processData: false,
                    async: false,
                    data: data,
                    success: function (result) {
                        debugger;
                        if (result.Result == "1") {
                            SuccessAlert("Image uploaded successfully");
                            window.location.href = "/NFCProfile/EditProfileSetAction?id=" + $("#Code").val();
                        }
                    },
                    error: function (xhr, status, p3, p4) {
                        var err = "Error " + " " + status + " " + p3 + " " + p4;
                        if (xhr.responseText && xhr.responseText[0] == "{")
                            err = JSON.parse(xhr.responseText).Message;
                        ErrorAlert(err);
                    }
                });
            } else {
                ErrorAlert("Not Uploaded");
            }
        }
    });
