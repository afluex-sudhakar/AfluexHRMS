            <div class="col-xl-3 col-lg-4">
                <div class="clearfix">
                    <div class="card card-bx profile-card author-profile m-b30">
                        <div class="card-body">
                            <div class="p-3">
                                <div class="author-profile">
                                    <div class="author-media">
                                        @*<img src="@ViewBag.img" alt="">*@
                                        <img src="~/assets/img/User.jpg" alt="">
                                        <div class="upload-link" title="" data-toggle="tooltip" data-placement="right" data-original-title="update">
                                            <input type="file" class="update-flie">
                                            <i class="fa fa-camera"></i>
                                        </div>
                                    </div>
                                    <div class="author-info mb-3">
                                        <h6 class="title">@Session["Name"]</h6>
                                    </div>


                                    <div class="form-row mb-4">
                                        <div class="col-lg-12 col-md-12 col-sm-12 my-1 d-flex">
                                            <div class="input-group input-group-sm copy">
                                                @*<input id="txtcopy" type="text" class="form-control input-default" readonly value="https://dost.click/NFC/Profile?id=@Model.Code">*@
                                                <input id="txtcopy" type="text" class="form-control input-default" readonly value="http://dost.click?id=@Model.enc">

                                                <a href="javascript:void(0);" class="btn btn-primary light me-1 px-3 close" style="padding-top: 11px;"><i class="fa fa-link m-0"></i> </a>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="single_media_card">
                                <div class="text-center">
                                    <div class="my-profile mb-2">
                                        <img src="@ViewBag.QRImage" alt="" style="border: 2px solid white; border-radius: 8px; width: 200px; height: 200px;">
                                    </div>
                                    <p class="text-justify px-3">Anyone can scan this QR to access  your profile as per your settings & send money to your Afluex HRMS wallet.</p>
                                </div>

                                <div class="text-center">
                                    <div class="post-input">
                                        <button type="button" class="btn btn-primary text-center" data-bs-toggle="modal" data-bs-target="#exampleModalCentersharelink">Share Profile</button>
                                        <a href="/NFCProfile/EditProfileSetAction?Id=@Model.Code" class="btn btn-primary text-center">Edit Profile</a>


                                        
                                        <div class="modal fade" id="exampleModalCentersharelink">
                                            <div class="modal-dialog modal-dialog-centered" role="document">
                                                <div class="modal-content">
                                                    <div class="modal-header">
                                                        <h5 class="modal-title">Business Card</h5>
                                                        <button type="button" class="btn-close" data-bs-dismiss="modal"></button>
                                                    </div>
                                                    <div class="modal-body">
                                                        <div class="form-row ">
                                                            @if (Model != null)
                                                            {
                                                                if (Model.NfcContentList != null && Model.NfcContentList.Count > 0 && Model.NfcContentList.Where(x => x.Type == "ContactNo").Any())
                                                                {
                                                                    foreach (var item in Model.NfcContentList.Where(x => x.Type == "ContactNo").ToList())
                                                                    {
                                                                        if (item.Content.Contains("6") || item.Content.Contains("7") || item.Content.Contains("9") || item.Content.Contains("8"))
                                                                        {
                                                                            <a class="btn-social whatsapp" href="https://api.whatsapp.com/send?phone=91+@item.Content&text=https://dost.click?id=@Model.enc" target="_blank"><i class="fa fa-whatsapp"></i></a>
                                                                        }
                                                                    }
                                                                }
                                                            }
                                                            @if (Model != null)
                                                            {
                                                                if (Model.NfcContentList != null && Model.NfcContentList.Count > 0 && Model.NfcContentList.Where(x => x.Type == "SocialMedia").Any())
                                                                {
                                                                    foreach (var item in Model.NfcContentList.Where(x => x.Type == "SocialMedia").ToList())
                                                                    {
                                                                        if (item.Content.Contains("facebook") || item.Content.Contains("fb"))
                                                                        {
                                                                            <a class="btn-social facebook" href="@item.Content" target="_blank"><i class="fa fa-facebook"></i></a>
                                                                        }
                                                                        else
                                                                        {
                                                                            <a class="btn-social facebook" href="https://www.facebook.com/" target="_blank"><i class="fa fa-facebook"></i></a>
                                                                        }
                                                                        if (item.Content.Contains("instagram") || item.Content.Contains("insta"))
                                                                        {
                                                                            <a class="btn-social instagram" href="@item.Content" target="_blank"><i class="fa fa-instagram"></i></a>
                                                                        }
                                                                        else
                                                                        {
                                                                            <a class="btn-social instagram" href="https://www.instagram.com/?hl=en" target="_blank"><i class="fa fa-instagram"></i></a>
                                                                        }
                                                                        if (item.Content.Contains("linkedin") || item.Content.Contains("link"))
                                                                        {
                                                                            <a class="btn-social linkedin" href="@item.Content" target="_blank"><i class="fa fa-linkedin"></i></a>
                                                                        }
                                                                        else
                                                                        {
                                                                            <a class="btn-social linkedin" href="https://www.linkedin.com/login?fromSignIn=true&trk=guest_homepage-basic_nav-header-signin" target="_blank"><i class="fa fa-linkedin"></i></a>
                                                                        }
                                                                        if (item.Content.Contains("twitter") || item.Content.Contains("tw"))
                                                                        {
                                                                            <a class="btn-social twitter" href="@item.Content" target="_blank"><i class="fa fa-twitter"></i></a>
                                                                        }
                                                                        else
                                                                        {
